{
  "name": "santelinkweb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode serve-dev --host",
    "build": "vite build --mode build",
    "preview": "vite preview",
    "build:android": "npm run build && npx cap sync android",
    "build:apk": "npm run build:android && cd android && ./gradlew assembleDebug",
    "build:apk-release": "npm run build:android && cd android && ./gradlew assembleRelease",
    "clean:android": "cd android && ./gradlew clean",
    "sync:android": "npx cap sync android",
    "open:android": "npx cap open android"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/app": "^7.0.2",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/keyboard": "^7.0.2",
    "@element-plus/icons-vue": "0.2.4",
    "axios": "^1.11.0",
    "element-plus": "^2.11.1",
    "jwt-decode": "^3.1.2",
    "mitt": "3.0.0",
    "nprogress": "0.2.0",
    "vue": "3.2.26",
    "vue-router": "4.0.12",
    "vuex": "4.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "1.3.1",
    "@vue/compiler-sfc": "3.2.26",
    "sass": "^1.91.0",
    "sass-embedded": "^1.91.0",
    "typescript": "4.3.2",
    "vite": "^7.1.3",
    "vite-plugin-svg-icons": "^0.1.0",
    "vue-tsc": "0.28.1"
  }
}
